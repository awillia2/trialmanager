    <fields>
        <fullName>TrialOrderStatus__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Order__r.Status)</formula>
        <label>Trial Order Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
        <inlineHelpText> Provisioning status of this request. Activated=Order placed, not provisioned; Pending=Order is in the provisioning queue; Provsioned=the request is complete, and the feature should be available in the trial org.</inlineHelpText>
    </fields>
    <fields>
        <fullName>Edition__c</fullName>
        <externalId>false</externalId>
        <label>Edition</label>
        <length>50</length>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>    <fields>
        <fullName>TotalProvisionedCoreLicenses__c</fullName>
        <externalId>false</externalId>
        <label>Total Provisioned Core Licenses</label>
        <summarizedField>tmTrialFeatureRequestItem__c.ProvisionedQuantity__c</summarizedField>
        <summaryFilterItems>
            <field>tmTrialFeatureRequestItem__c.IsCoreLicense__c</field>
            <operation>equals</operation>
            <value>True</value>
        </summaryFilterItems>
        <summaryForeignKey>tmTrialFeatureRequestItem__c.TrialFeatureRequest__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackHistory>false</trackHistory>
        <inlineHelpText> The total number of core licenses that had been provisioned at the time of the request. NOTE: this may not indicate the current number of licenses provisioned. Please view the customer's request history to determine the total number of licenses.</inlineHelpText>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>TotalRequestedAddonLicenses__c</fullName>
        <externalId>false</externalId>
        <label>Total Requested Addon Licenses</label>
        <summarizedField>tmTrialFeatureRequestItem__c.RequestedQuantity__c</summarizedField>
        <summaryFilterItems>
            <field>tmTrialFeatureRequestItem__c.RequestedQuantity__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </summaryFilterItems>
        <summaryFilterItems>
            <field>tmTrialFeatureRequestItem__c.IsCoreLicense__c</field>
            <operation>equals</operation>
            <value>False</value>
        </summaryFilterItems>
        <summaryFilterItems>
            <field>tmTrialFeatureRequestItem__c.RequiresApproval__c</field>
            <operation>equals</operation>
            <value>True</value>
        </summaryFilterItems>
        <summaryForeignKey>tmTrialFeatureRequestItem__c.TrialFeatureRequest__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackHistory>false</trackHistory>
        <inlineHelpText> Total quantity of addon product licenses on this request.</inlineHelpText>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>TotalRequestedCoreLicenses__c</fullName>
        <externalId>false</externalId>
        <label>Total Requested Core Licenses</label>
        <summarizedField>tmTrialFeatureRequestItem__c.RequestedQuantity__c</summarizedField>
        <summaryFilterItems>
            <field>tmTrialFeatureRequestItem__c.IsCoreLicense__c</field>
            <operation>equals</operation>
            <value>True</value>
        </summaryFilterItems>
        <summaryForeignKey>tmTrialFeatureRequestItem__c.TrialFeatureRequest__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <trackHistory>false</trackHistory>
        <type>Summary</type>
        <inlineHelpText> Total number of additional core licenses on this request.</inlineHelpText>
    </fields>
    <fields>
        <fullName>TotalCoreLicenses__c</fullName>
        <externalId>false</externalId>
        <formula>TotalProvisionedCoreLicenses__c  +  TotalRequestedCoreLicenses__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Core Licenses</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <inlineHelpText> Total number of core licenses that will be provisioned after this request is complete. (Total Provisioned Core Licenses+Total Requested Core Licenses)</inlineHelpText>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TrialOrderType__c</fullName>
        <externalId>false</externalId>
        <label>Trial Order Type</label>
        <picklist>
            <picklistValues>
                <fullName>New</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Addon</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Upgrade</fullName>
                <default>false</default>
            </picklistValues>
        </picklist>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
